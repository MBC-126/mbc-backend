{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": "Événements à venir sur la base"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "eventDate": {
      "type": "datetime",
      "required": false
    },
    "date": {
      "type": "string",
      "required": false,
      "maxLength": 50
    },
    "time": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "location": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
